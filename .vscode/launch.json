{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest via pnpm",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "args": ["jest", "test/software-development/base-handler.test.ts", "--runInBand", "--no-cache", "-t idempotency"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
